<?xml version="1.0"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="robot">

 <!-- include materials /-->
 <xacro:include filename="$(find robot_description)/model/materials.xacro" />

  <!-- World -->
  <link name="world_link">
  </link>

  <!-- World fixed joint-->
  <joint name="world_fixed_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="world_link"/>
    <child link="base_link"/>
  </joint>
  


  <link name="base_link">
    
    <inertial>
        <mass value="1.488"/>
        <origin xyz="0 0 0.068"  />
        <inertia ixx="0.0047" ixy="0.0" ixz="0.0047" iyy="0." iyz="0.0" izz="0.053"/>
    </inertial> 

    <collision>
      <origin xyz="0 0 0.073" rpy="0 0 0"/>
      <geometry>
        <!--mesh filename="package://niki_arm_description/meshes/base_link2.stl" scale="0.01 0.01 0.01"/ -->
        <box size="0.146 0.146 0.129"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0.073" rpy="0 0 0"/>
      <geometry>
        <!-- <mesh filename="package://niki_arm_description/meshes/base_link2.stl" scale="0.01 0.01 0.01"/> -->
        <box size="0.146 0.146 0.129"/>
      </geometry>
      <material name="white"/>
    </visual>

  </link>

  <!-- Joint BaseToJoint1 -->
	<joint name="link1_fixed_joint" type="fixed">
		<parent link="base_link"/>
		<child link="link_1"/>
		<origin xyz="0 0 0.137" rpy="0 0 0"/>
		<!-- <axis xyz="0 0 1"/> -->
		<!-- <limit velocity="4.8" effort="1" lower="${-pi*0.9}" upper="${pi*0.9}" /> -->
	</joint>

	<!-- link_1 -->
  <link name="link_1">

     <inertial>
        <mass value="0.165"/>
        <origin xyz="0 0.024 0.0205"  />
        <inertia ixx="0.00007" ixy="0.0" ixz="0.0" iyy="0.00005" iyz="0.0" izz="0.0001"/>
    </inertial>

		<visual>
			<origin xyz="0 0 0.0205" rpy="0 0 0"/>
			<geometry>
				<mesh filename="package://robot_description/meshes/mx-64base.stl" scale="0.01 0.01 0.01"/>
			</geometry>
			<material name="blue"/>
		</visual>

		<collision>
			<origin xyz="0 0 0.0205" rpy="0 0 0"/>
			<geometry>
				<mesh filename="package://robot_description/meshes/mx-64base.stl" scale="0.01 0.01 0.01"/>
			</geometry>
		</collision>
	</link>	

  <!-- joint1 -->
  <joint name="joint1" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0 0.0 0.054" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="4.8" effort="1" lower="-1.56" upper="1.56" />
  </joint>

  <!-- link_2 -->
  <link name="link_2">

    <inertial>
        <mass value="0.165"/>
        <origin xyz="0 0.0 -0.0175"  />
        <inertia ixx="0.00007" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.00005"/>
    </inertial>

    <visual>
      <origin xyz="0 0 0.048" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robot_description/meshes/mx-64t2.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="red"/>
    </visual>

    <collision>
      <origin xyz="0 0 0.048" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robot_description/meshes/mx-64t2.stl" scale="0.01 0.01 0.01"/>
      </geometry>
    </collision>
  </link> 

  <!-- joint2 -->
  <joint name="joint2" type="revolute">
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin xyz="0 0.0 0.048" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="4.8" effort="1" lower="-1.56" upper="1.56" />
  </joint>

  <!-- link_3 -->
  <link name="link_3">

    <inertial>
        <mass value="0.215"/>
        <origin xyz="0 0.0 0.100"  />
        <inertia ixx="0.00004" ixy="0.0" ixz="0.0" iyy="0.00004" iyz="0.0" izz="0.00007"/>
    </inertial>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robot_description/meshes/linked_mx_64t.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="green"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robot_description/meshes/linked_mx_64t.stl" scale="0.01 0.01 0.01"/>
      </geometry>
    </collision>
  </link> 

  <!-- joint3 -->
  <joint name="joint3" type="revolute">
    <parent link="link_3"/>
    <child link="link_4"/>
    <origin xyz="0 0.0 0.12" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="4.8" effort="1" lower="-1.56" upper="1.56" />
  </joint>

  <!-- link_4 -->
  <link name="link_4">

    <inertial>
        <mass value="0.215"/>
        <origin xyz="0 0.0 0.100"  />
        <inertia ixx="0.00004" ixy="0.0" ixz="0.0" iyy="0.00004" iyz="0.0" izz="0.00007"/>
    </inertial>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robot_description/meshes/linked_mx_64t.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="orange"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robot_description/meshes/linked_mx_64t.stl" scale="0.01 0.01 0.01"/>
      </geometry>
    </collision>
  </link> 

  <!-- joint4 -->
  <joint name="joint4" type="revolute">
    <parent link="link_4"/>
    <child link="endeffector"/>
    <origin xyz="0 0 0.12" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="4.8" lower="-1.56" upper="1.56" velocity="10"/>
  </joint>

  <!-- EndEffector -->
  <link name="endeffector">
    <collision>
      <origin xyz="0 0 0.057" rpy="0 0 0"/>
      <geometry>
        <!--mesh filename="package://niki_arm_description/meshes/base_link2.stl" scale="0.01 0.01 0.01"/ -->
        <box size="0.05 0.026 0.114"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0.057" rpy="0 0 0"/>
      <geometry>
        <!-- <mesh filename="package://niki_arm_description/meshes/base_link2.stl" scale="0.01 0.01 0.01"/> -->
        <box size="0.05 0.026 0.114"/>
      </geometry>
      <material name="white"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="0.050"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>
   
   </robot>
